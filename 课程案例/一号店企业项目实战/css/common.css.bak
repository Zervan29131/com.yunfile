/*common部分*/
body,
div,
ul,
li,
p,
input,
button{
	margin:0px;
	padding:0px;
}

html,
body{
	width:100%;
	height:100%;
	font-size:12px; 
	font-family:"微软雅黑",Arial,Helvetica,sans-serif;
	color:#333;
}

a{
	text-decoration:none;
	color:#666;
}

li{
	list-style:none;
}

.wrap{
	width:100%;
	background:#f9f9f9;
}

.content{
	width:1200px;
	margin:0 auto;
}

.fl{
	float:left;
}

.fr{
	float:right;
}
.clearLine{
	height:1px;
	position:absolute;
	background:#fff;
	display:none;
}
.clearfix:after{
	content:"";
}
